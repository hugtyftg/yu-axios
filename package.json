{
  "name": "ts-lib",
  "type": "module",
  "version": "0.0.4",
  "license": "MIT",
  "exports": {
    "types": "./index.d.ts",
    "import": "./dist/ts-lib.mjs",
    "require": "./dist/ts-lib.umd.js"
  },
  "main": "./dist/ts-lib.umd.js",
  "module": "./dist/ts-lib.mjs",
  "types": "./index.d.ts",
  "files": [
    "README.md",
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "dev": "vite serve example",
    "lint": "eslint --fix",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && vite build",
    "test": "vitest --coverage --run",
    "test:watch": "vitest --coverage",
    "test:prod": "npm run lint && vitest --coverage --run",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "commit": "git-cz",
    "prepub": "npm run test:prod && npm run build",
    "pub": "sh release.sh"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/node": "^18.17.0",
    "@vitest/coverage-v8": "^1.6.0",
    "commitizen": "4.2.4",
    "cz-customizable": "6.3.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-prettier": "^5.4.1",
    "jsdom": "^22.1.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.4.5",
    "vite": "^5.3.5",
    "vitepress": "^1.3.1",
    "vitest": "^1.6.0"
  }
}
